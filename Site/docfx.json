{
  "metadata": [
    {
      "src": [
        {
          "src": "C:\\Projects\\Unity\\rwbyq\\rwbyq_unity",
          "files": [
            "**/Assembly-CSharp.csproj",
            "**/rwby.csproj",
            "**/rwby.editor.csproj",
            "**/rwby.core.view.csproj",
            "**/rwby.modding.umod.csproj",
            "**/rwby.modding.helper.csproj",
            "**/EQS.Editor.csproj",
            "**/HNSF.Editor.csproj",
            "**/Quantum.Simulation.csproj"
          ],
		  "exclude": [
			"**/Assets/Photon/**"
		  ]
        }
      ],
	  "globalNamespaceId": "Global",
	  "filter": "filterConfig.yml",
      "dest": "api",
	  "properties": { "AllowUnsafeBlocks": true },
	  "allowCompilationErrors": true
    }
  ],
  "build": {
    "globalMetadata": {
      "_appName": "Rwby Vytal Versus Documentation",
      "_appTitle": "Rwby Vytal Versus Documentation",
      "_enableSearch": true,
      "pdf": false
    },
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
	"sitemap":
	{
		"baseUrl": "https://christides11.github.io/RwbyVytalVersus/",
		"changefreq": "weekly",
		"fileOptions": {
			"api/*": {
				"changefreq": "daily"
			}
		}
	},
	"xref": [ "https://normanderwan.github.io/UnityXrefMaps/xrefmap.yml" ],
	"xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
    "output": "../docs",
    "template": [
      "default",
      "modern"
    ],
  }
}