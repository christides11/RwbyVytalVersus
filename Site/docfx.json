{
  "metadata": [
    {
      "src": [
        {
          "src": "C:\\Projects\\Unity\\rwbyq\\rwbyq_unity",
          "files": [
            "**/rwby.csproj",
            "**/rwby.editor.csproj"
          ]
        }
      ],
	  "globalNamespaceId": "Global",
	  "filter": "filterConfig.yml",
      "dest": "api",
	  "properties": { "AllowUnsafeBlocks": true }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Rwby Vytal Versus Documentation",
      "_appTitle": "Rwby Vytal Versus Documentation",
      "_enableSearch": true,
      "pdf": true
    },
	"xref": [ "https://normanderwan.github.io/UnityXrefMaps/xrefmap.yml" ],
	"xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
  }
}